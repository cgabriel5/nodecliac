{
	"name": "acmap",
	"displayName": "acmap",
	"description": "Adds grammar support for .acmap/.acdef files",
	"version": "0.0.1",
	"publisher": "cgabriel5",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "acmap",
				"aliases": [
					"ACMAP",
					"acmap"
				],
				"extensions": [
					".acmap",
					".acdef"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "acmap",
				"scopeName": "source.acmap",
				"path": "./syntaxes/acmap.tmLanguage.json"
			}
		]
	}
}
