{
	"name": "nodecliac",
	"version": "0.0.1",
	"description": "Add Bash auto completion to your CLI app.",
	"main": "./src/index.js",
	"bin": {
		"nodecliac": "./src/index.js"
	},
	"bugs": {
		"url": "https://github.com/cgabriel5/nodecliac/issues"
	},
	"homepage": "https://github.com/cgabriel5/nodecliac#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cgabriel5/nodecliac.git"
	},
	"scripts": {
		"postinstall": "./src/scripts/main/postinstall.sh",
		"pretty": "prettier-cli-watcher --configpath='/configs/prettier.config.json'",
		"lint": "cross-env ESLINT_CFS_VERBOSE=true ./node_modules/eslint/bin/eslint.js -c='configs/eslint.config.js' --ignore-path='configs/eslintignore' --format='simple' '**/*.{js,jsx}'; exit 0",
		"tests": "tests/scripts/main.sh",
		"tests:silent": "tests/scripts/main.sh -p false",
		"tests:force": "tests/scripts/main.sh -f true",
		"build:acbin": "src/scripts/main/compile.sh -i src/scripts/ac/ac.nim",
		"build:acbin": "bash -c \"OS=`uname`;USER_OS=\\\"\\${OS,,}\\\";USER_OS=\\\"\\${USER_OS/darwin/macosx}\\\"; src/scripts/main/compile.sh -i src/scripts/ac/ac.nim \\$0 && mv src/scripts/ac/ac.\\\"\\$USER_OS\\\" src/scripts/bin/ac.\\\"\\$USER_OS\\\"\"",
		"build:nimbin": "bash -c \"OS=`uname`;USER_OS=\\\"\\${OS,,}\\\";USER_OS=\\\"\\${USER_OS/darwin/macosx}\\\"; src/scripts/main/compile.sh -i src/parser/nim/index.nim \\$0 && mv src/parser/nim/index.\\\"\\$USER_OS\\\" src/parser/nim/nodecliac.\\\"\\$USER_OS\\\"\"",
		"files": "bash -c \"find . -not -path \\\"*/node_modules/*\\\" -type f -name \\\"*.\\$0\\\" -not -path '*/\\\\.*'\"",
		"install:binary": "bash -c \"s=\\\"\\$(cat ./install)\\\"; echo \\\"\\$s\\\" | bash -s -- --installer=binary --branch=\\$0 && source ~/.bashrc\""
	},
	"dependencies": {
		"chalk": "^3.0.0",
		"directory-exists": "^2.0.1",
		"fancy-log": "^1.3.3",
		"file-exists": "^5.0.1",
		"file-extension": "^4.0.5",
		"flatry": "^1.0.12",
		"make-dir": "^3.0.0",
		"minimist": "^1.2.0",
		"path-exists": "^4.0.0",
		"recursive-copy": "^2.0.10",
		"rimraf": "^3.0.0",
		"shelljs": "^0.8.3",
		"strip-ansi": "^6.0.0",
		"symlink-dir": "^3.1.1",
		"through2": "^3.0.1"
	},
	"author": "Carlos Gabriel",
	"license": "MIT",
	"keywords": [
		"bash",
		"cli",
		"terminal",
		"completion"
	],
	"engines": {
		"node": ">=8.14.0",
		"npm": ">=6.4.1"
	},
	"devDependencies": {
		"cross-env": "^6.0.3",
		"eslint": "^6.6.0",
		"eslint-config-simple": "*",
		"eslint-formatter-simple": "*",
		"prettier": "^1.19.1",
		"prettier-cli-watcher": "*"
	},
	"files": [
		"src",
		"resources/nodecliac"
	]
}
