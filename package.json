{
	"name": "nodecliac",
	"version": "0.0.1",
	"description": "Add Bash auto completion to your CLI app.",
	"main": "./src/index.js",
	"bin": {
		"nodecliac": "./src/index.js"
	},
	"bugs": {
		"url": "https://github.com/cgabriel5/nodecliac/issues"
	},
	"homepage": "https://github.com/cgabriel5/nodecliac#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cgabriel5/nodecliac.git"
	},
	"scripts": {
		"postinstall": "./src/scripts/main/postinstall.sh",
		"pretty": "prettier-cli-watcher --watcher=hound --notify --setup",
		"lint": "cross-env ESLINT_CFS_VERBOSE=true ./node_modules/eslint/bin/eslint.js -c='configs/eslint.config.js' --ignore-path='configs/eslintignore' --format='simple' '**/*.{js,jsx}'; exit 0",
		"tests": "tests/scripts/main.sh -t ./tests/scripts/tests.sh -n $1",
		"tests:silent": "tests/scripts/main.sh -p false -t ./tests/scripts/tests.sh -n $1",
		"tests:force": "tests/scripts/main.sh -f true -t ./tests/scripts/tests.sh -n $1",
		"build:acbin": "bash -c \"OS=`uname`;F=\\\"\\${1:-ac}\\\";USER_OS=\\\"\\${OS,,}\\\";USER_OS=\\\"\\${USER_OS/darwin/macosx}\\\"; src/scripts/main/compile.sh -i src/scripts/ac/\\\"\\$F.\\\"nim \\$0 && mv src/scripts/ac/\\\"\\$F.\\\"\\\"\\$USER_OS\\\" src/scripts/bin/\\\"\\$F.\\\"\\\"\\$USER_OS\\\"\"",
		"build:nimbin": "bash -c \"OS=`uname`;USER_OS=\\\"\\${OS,,}\\\";USER_OS=\\\"\\${USER_OS/darwin/macosx}\\\"; src/scripts/main/compile.sh -i src/parser/nim/index.nim \\$0 && mv src/parser/nim/index.\\\"\\$USER_OS\\\" src/parser/nim/nodecliac.\\\"\\$USER_OS\\\"\"",
		"files": "bash -c \"find . -not -path \\\"*/node_modules/*\\\" -type f -name \\\"*.\\$0\\\" -not -path '*/\\\\.*'\"",
		"install:local": "sudo echo > /dev/null 2>&1 && bash -c \"s=\\\"\\$(<./install.sh)\\\"; echo \\\"\\$s\\\" | bash -s -- --installer=\\$0 --branch=\\$1 && source ~/.bashrc\"",
		"install:remote": "sudo echo > /dev/null 2>&1 && bash -c \"curl -Ls https://raw.githubusercontent.com/cgabriel5/nodecliac/\\$1/install.sh | bash -s -- --installer=\\$0 --branch=\\$1 && source ~/.bashrc\"",
		"checksum": "./src/scripts/main/checksum.sh"
	},
	"dependencies": {
		"chalk": "^4.1.0",
		"deepcopy": "^2.0.0",
		"directory-exists": "^2.0.1",
		"du": "^1.0.0",
		"file-exists": "^5.0.1",
		"file-extension": "^4.0.5",
		"flatry": "^1.0.12",
		"make-dir": "^3.1.0",
		"minimist": "^1.2.5",
		"path-exists": "^4.0.0",
		"recursive-copy": "^2.0.10",
		"rimraf": "^3.0.2",
		"shelljs": "^0.8.4",
		"strip-ansi": "^6.0.0",
		"symlink-dir": "^4.1.0",
		"through2": "^4.0.2"
	},
	"author": "Carlos Gabriel",
	"license": "MIT",
	"keywords": [
		"bash",
		"cli",
		"terminal",
		"completion"
	],
	"engines": {
		"node": ">=8.14.0",
		"npm": ">=6.4.1"
	},
	"devDependencies": {
		"cross-env": "^7.0.2",
		"eslint": "^7.5.0",
		"eslint-config-simple": "*",
		"eslint-formatter-simple": "*",
		"prettier": "^2.0.5",
		"prettier-cli-watcher": "*"
	},
	"files": [
		"src",
		"resources/nodecliac"
	]
}
