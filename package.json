		"rimraf": "^2.6.3",
		"strip-ansi": "^5.2.0"
	},
	"author": "Carlos Gabriel",
	"license": "MIT",
	"keywords": [
		"bash",
		"cli",
		"terminal",
		"completion"
	],
	"engines": {
		"node": ">=8.14.0",
		"npm": ">=6.4.1"
	},
	"devDependencies": {
		"cross-env": "^5.2.0",
		"eslint": "^5.12.0",
		"eslint-config-simple": "^0.0.2",
		"eslint-formatter-simple": "^0.0.3",
		"prettier-cli-watcher": "^0.3.1"
	}
}
