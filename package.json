{
	"name": "nodecliac",
	"version": "0.0.1",
	"description": "Add Bash auto completion to your CLI app.",
	"main": "./src/index.js",
	"bin": {
		"nodecliac": "./src/index.js"
	},
	"bugs": {
		"url": "https://github.com/cgabriel5/nodecliac/issues"
	},
	"homepage": "https://github.com/cgabriel5/nodecliac#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cgabriel5/nodecliac.git"
	},
	"scripts": {
		"pretty": "prettier-cli-watcher --configpath='/configs/prettier.config.json'",
		"lint": "cross-env ESLINT_CFS_VERBOSE=true ./node_modules/eslint/bin/eslint.js -c='configs/eslint.config.js' --ignore-path='configs/eslintignore' --format='simple' '**/*.{js,jsx}'; exit 0",
		"tests": "tests/nodecliac.sh && tests/parser.sh && tests/formatter.sh",
		"tests:silent": "tests/nodecliac.sh -s && tests/parser.sh -s && tests/formatter.sh -s",
		"build:acdev": "./compile -d",
		"build:acprod": "./compile -p"
	},
	"dependencies": {
		"chalk": "^2.4.1",
		"directory-exists": "^2.0.1",
		"fancy-log": "^1.3.3",
		"file-exists": "^5.0.1",
		"file-extension": "^4.0.5",
		"flatry": "^1.0.12",
		"make-dir": "^3.0.0",
		"minimist": "^1.2.0",
		"path-exists": "^4.0.0",
		"recursive-copy": "^2.0.10",
		"rimraf": "^3.0.0",
		"shelljs": "^0.8.3",
		"strip-ansi": "^5.2.0",
		"through2": "^3.0.1"
	},
	"author": "Carlos Gabriel",
	"license": "MIT",
	"keywords": [
		"bash",
		"cli",
		"terminal",
		"completion"
	],
	"engines": {
		"node": ">=8.14.0",
		"npm": ">=6.4.1"
	},
	"devDependencies": {
		"cross-env": "^5.2.0",
		"eslint": "^6.6.0",
		"eslint-config-simple": "^0.0.7",
		"eslint-formatter-simple": "^0.0.7",
		"prettier": "^1.19.1",
		"prettier-cli-watcher": "^0.3.4"
	},
	"files": [
		"src",
		"resources/nodecliac"
	]
}
